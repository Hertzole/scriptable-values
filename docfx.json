{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "./",
          "files": [
            "Hertzole.ScriptableValues.csproj",
            "Hertzole.ScriptableValues.Editor.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "DefineConstants": "UNITY_EDITOR;UNITY_2022_3_OR_NEWER;SCRIPTABLE_VALUES_RUNTIME_BINDING;SCRIPTABLE_VALUES_PROPERTIES;SCRIPTABLE_VALUES_ADDRESSABLES"
      },
      "xref": [
        "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "NO_SITE",
      "_appTitle": "NO_SITE",
      "_enableSearch": false,
      "pdf": false
    },
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ]
  }
}